<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - Smart Tracking</title>
    <!-- Bootstrap core CSS -->
    <link href="@Url.Content("~/Content/Templates/Gentelella Alela/css/bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/Templates/Gentelella Alela/fonts/css/font-awesome.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/Templates/Gentelella Alela/css/animate.min.css")" rel="stylesheet">
    <!-- Custom styling plus plugins -->
    <link href="@Url.Content("~/Content/Templates/Gentelella Alela/css/custom.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/Templates/Gentelella Alela/css/icheck/flat/green.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Content/Templates/Gentelella Alela/js/jquery.min.js")"></script>

</head>
<body class="nav-md">
    <div class="container body">

        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="~/" class="site_title"><i class="fa fa-paw"></i> <span>Smart Tracking</span></a>
                    </div>
                    <div class="clearfix"></div>
                    <!-- menu prile quick info -->
                    @RenderPage("~/Views/Shared/Partial Pages/_Profile.cshtml")
                    <!-- /menu prile quick info -->
                    <br />
                    <!-- sidebar menu -->
                    @RenderPage("~/Views/Shared/Partial Pages/_Menu.cshtml")
                    <!-- /sidebar menu -->
                </div>
            </div>
            <!-- top navigation -->
            @RenderPage("~/Views/Shared/Partial Pages/_TopNavigation.cshtml")
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">
                @RenderBody()
                <!-- footer content -->
                @RenderPage("~/Views/Shared/Partial Pages/_Footer.cshtml")
                <!-- /footer content -->
            </div>
            <!-- /page content -->
        </div>
    </div>
    @RenderPage("~/Views/Shared/Partial Pages/_Notifications.cshtml")


    <script src="@Url.Content("~/Content/Templates/Gentelella Alela/js/bootstrap.min.js")"></script>

    <!-- bootstrap progress js -->
    <script src="@Url.Content("~/Content/Templates/Gentelella Alela/js/progressbar/bootstrap-progressbar.min.js")"></script>
    <script src="@Url.Content("~/Content/Templates/Gentelella Alela/js/nicescroll/jquery.nicescroll.min.js")"></script>
    <!-- icheck -->
    <script src="@Url.Content("~/Content/Templates/Gentelella Alela/js/icheck/icheck.min.js")"></script>
    <!-- pace -->
    <script src="@Url.Content("~/Content/Templates/Gentelella Alela/js/pace/pace.min.js")"></script>
    <script src="@Url.Content("~/Content/Templates/Gentelella Alela/js/custom.js")"></script>

</body>
</html>
