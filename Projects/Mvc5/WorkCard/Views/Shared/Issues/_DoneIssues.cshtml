@using CafeT.Time
@model IEnumerable<Web.ModelViews.IssueView>

<div class="dropdown pull-right">
    <a href="#" class="dropdown-toggle card-drop" data-toggle="dropdown" aria-expanded="false">
        <i class="zmdi zmdi-more-vert"></i>
    </a>
    <ul class="dropdown-menu" role="menu">
        <li><a href="#">Action</a></li>
        <li><a href="#">Another action</a></li>
        <li><a href="#">Something else here</a></li>
        <li class="divider"></li>
        <li><a href="#">Separated link</a></li>
    </ul>
</div>
<ul class="list-unstyled task-list" id="drag-upcoming">
    @foreach (var item in Model)
    {
        <li class="list-group-item">
            @Html.Partial("Issues/_DoneIssueItem", item)
        </li>
    }
</ul>
<div class="text-center">
    <a href="#custom-modal" class="btn btn-default btn-md waves-effect waves-light"
       data-animation="fadein" data-plugin="custommodal" data-overlaySpeed="200" data-overlayColor="#36404a">
        <i class="zmdi zmdi-plus"></i> Add New
    </a>
</div>