@model Mvc5.CafeT.vn.ModelViews.ArticleView

@Html.Raw("Google to Drives")
<hr />
@using (Html.BeginForm("UploadGoogleFileAsync", "FileModels", new { articleId = Model.Id },
                FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <fieldset>
        <div class="editor-field">
            <input type="file" id="uploadFile" name="file" multiple="multiple" />
        </div>
        <div class="editor-field">
            <input type="submit" value="Upload" />
        </div>
    </fieldset>
}